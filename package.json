    "prisma": "^6.5.0",
    "typescript": "^5.8.2"
  },
  "dependencies": {
    "@prisma/client": "^6.5.0",
    "@vercel/node": "^5.1.14",
    "cors": "^2.8.5",
    "express": "^4.21.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0"
  }
